{
  "extends": "google",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules" : {
    "max-len": [1,120],
    "require-jsdoc": [1],
    "class-methods-use-this": [0],
    "quotes": [0],
    "linebreak-style": [0],
    "no-trailing-spaces": [0]
  }
}